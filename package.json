    "check:type-coverage": "type-coverage --detail --strict --at-least 99 --ignore-files 'test/*'",
    "check": "run-p check:*",
    "prepare": "husky",
    "test": "run-s check"
  },
  "devDependencies": {
    "@types/node": "^18.19.22",
    "@voxpelli/tsconfig": "^11.0.0",
    "husky": "^9.1.7",
    "installed-check": "^9.3.0",
    "knip": "^5.59.1",
    "npm-run-all2": "^7.0.2",
    "type-coverage": "^2.27.1",
    "typescript": "~5.4.2",
    "validate-conventional-commit": "^1.0.4"
  }
}
