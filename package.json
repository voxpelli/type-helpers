{
  "name": "@voxpelli/type-helpers",
  "version": "4.0.0",
  "description": "My personal type helpers",
  "homepage": "http://github.com/voxpelli/type-helpers",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/type-helpers.git"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": "^20.19.0 || ^22.13.0 || >=24",
    "typescript": ">=5.8"
  },
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "/index.d.ts",
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "scripts": {
    "check:installed-check": "installed-check -i tstyche",
    "check:knip": "knip",
    "check:lint": "eslint",
    "check:tsc": "tsc",
    "check:type-test": "tstyche",
    "check:type-coverage": "type-coverage --detail --strict --at-least 99 --ignore-files 'test/*'",
    "check": "run-p check:* --continue-on-error",
    "prepare": "husky",
    "test:tstyche": "tstyche --target \"$(jq -r '.engines.typescript' package.json)\"",
    "test": "run-s check test:*"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "@voxpelli/eslint-config": "^23.0.0",
    "@voxpelli/tsconfig": "^16.1.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "installed-check": "^10.0.0",
    "knip": "^5.83.1",
    "npm-run-all2": "^8.0.4",
    "tstyche": "^6.2.0",
    "type-coverage": "^2.29.7",
    "typescript": "~5.9",
    "validate-conventional-commit": "^1.0.4"
  }
}
