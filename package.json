  "engines": {
    "node": ">=16.0.0"
  },
  "types": "./index.d.ts",
  "files": [
    "/index.d.ts",
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "scripts": {
    "check:installed-check": "installed-check -i knip -i installed-check",
